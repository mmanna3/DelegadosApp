{
  "expo": {
    "name": "EDeFI-Delegados",
    "slug": "EDeFI-Delegados",
    "version": "2.1.0",
    "orientation": "portrait",
    "icon": "./assets/images/logo.png",
    "scheme": "myapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/images/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#01aa59"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "userInterfaceStyle": "light"
    },
    "android": {
      "package": "com.ykn.edefidelegados",
      //Si no, cuando el teclado es visible, tira otras cosas para arriba
      "softwareKeyboardLayoutMode": "pan", 
      "versionCode": 10,
      "userInterfaceStyle": "light"
    },
    "web": {
      "favicon": "./assets/images/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "d296a6cd-ac06-4c7c-8238-ff2c9b5339b6"
      }
    },
    "plugins": [
      "expo-font",[
        "expo-build-properties",
        {
          "android": {
            "compileSdkVersion": 33,
            "targetSdkVersion": 33,
            "buildToolsVersion": "33.0.0"
          }
        }
      ]
    ]
  }
}
